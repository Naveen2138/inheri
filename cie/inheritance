class Animal{
    void eat(){
        system.out.println("This animal eats food.");
    }
}
class dog extends Animal{
    void bark(){
        system.out.println("The dog barks.");
    }
}
public class SingleInheritance{
    public static void main(String[]args){
        Dog dog=new dog();
        dog.eat();
        dog.bark();
    }
}